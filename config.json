{
    "disks": {
      "local": {
        "driver": "local",
        "root": "C:/path"
      },
      "second_local": {
        "driver": "local",
        "root": "D:/path/2"
      },
      "s3_disk": {
        "driver": "s3",
        "s3": {
          "bucket": "my-bucket",
          "key": "my-access-key",
          "secret": "my-secret-key",
          "region": "us-west-2"
        }
      },
      "sftp_disk": {
        "driver": "sftp",
        "sftp": {
          "host": "example.com",
          "username": "myusername",
          "password": "mypassword",
          "port": 22
        }
      }
    }
  }
  
